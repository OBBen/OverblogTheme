			.item-comments {
				margin-top: 20px;
			}
			
			.single .social {
				clear: both;
			}
			.single .social h4 {
				margin-bottom: 10px;
			}
			
			.social .fb-like iframe {
				margin-top: -3px;
			}
			
			.navigation .prev,
			.navigation .next {
				color: #3A3A3A;
				margin: 20px 0;
			}
			
			.navigation a {
				display: inline-block;
				font-size: 16px;
			}
			.navigation a:first-letter,
			.navigation .prev:first-letter {
				text-transform: capitalize;
			}
			
			
			.navigation .prev {
				float: left;
			}
			
			.navigation .next {
				float: right;
			}
			
			.footer {
				{% if Custom('custom_hide_widget_search_form') == true %}
					margin: {% if isSingle or isSpecial %}0{% else %}-20px{% endif %} 0 20px;
				{% endif %}
	        	text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.4);
	        }
			.footer p,
			.footer a,
			.footer a:visited {
				color: {{ Custom('footer_color')|default('#F5F5F5') }};
				text-align: center;
			}
			.footer a:hover {
				text-decoration: underline;
			}
	        .footer a.credits {
	        	color: #ffae00 !important;
	        }
			
			.footer span {
				display: inline-block;
			}
			.footer span:first-letter {
				text-transform: capitalize;
			}
	
			.search {
				margin-left: auto;
				margin-right: auto;
				margin-bottom: 10px;
			}
	
			.search input[type=text]{
				border: none;
				border-radius: 5px;
				-webkit-box-shadow: 0 2px 6px rgba(0,0,0,0.4);
				   -moz-box-shadow: 0 2px 6px rgba(0,0,0,0.4);
				     -o-box-shadow: 0 2px 6px rgba(0,0,0,0.4);
				        box-shadow: 0 2px 6px rgba(0,0,0,0.4);
				padding: 5px;
			{% if Custom('background_item') != '#F5F5F5' %}

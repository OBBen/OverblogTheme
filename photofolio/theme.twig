                                    <h2>
                                        <a title="{{ Post.Snippet }}" href="{{ Post.PermaLink }}">
                                            {% if Post.Title is not empty %}
                                                {{ Post.Title }}
                                            {% else %}
                                                {{ Lang.Get('Published from') }} 
                                                {% list Post.Sources %}
                                                    {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
                                                {% endlist %}
                                            {% endif %}
                                        </a>
                                    </h2>
                                    <figure class="cover">
                                        <a class="" title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                            {% if Post.Cover is not empty %}
                                                <img alt="" src="{{ Post.Cover|resize(222,110)|crop }}" width="222" height="110" alt="" />
                                            {% elseif Custom('custom_default_post_cover') is not empty %}
                                                <img src="{{ Custom('custom_default_post_cover')|resize(222,110)|crop }}" width="222" height="110" alt="" />
                                            {% endif %}
                                        </a>
                                        <span class="comment-count">
                                            {% if Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
                                                <a title="{{ Lang.Get('comments') }}" href="{{ Blog.Url }}{{ Post.PermaLink }}#disqus_thread"></a>
                                            {% else %}
                                                <a title="{{ Lang.Get('comments') }}" href="{{ Post.PermaLink }}#comments">
                                                    {% if Custom("comment") == "Facebook" %}
                                                        <fb:comments-count href={{ Blog.Url }}{{ Post.Permalink }}></fb:comments-count> {{ Lang.Get('comment(s)') }}
                                                    {% else %}
                                                        {{ Post.CommentCount(Lang.Get('comment'), Lang.Get('comments')) }}
                                                    {% endif %}
                                                </a>
                                            {% endif %}
                                        </span>
                                    </figure>
                                    <p class="snippet">{{ Post.Snippet }}</p>
                                    <a class="button" title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                        {{ Lang.Get('Read more') }}
                                    </a>
                                </article>
                                {% if loop.index % 3 == 0 %}
                                    <div class="clear"></div>
                                {% endif %}
                            
                            {% else %}
                                {% if isSearch and Posts is empty %}
                                    <!-- No result for search -->
                                    <p class="search-null">{{ Lang.Get('No result for this expression') }}.</p>
                                {% endif %}
                            {% endlist %}
                        {% endif %}
                    </div>
              
                    <!-- Pagination -->
                    {% if Navigation.PreviousPage is not empty or Navigation.NextPage %}
                        <div class="pagination">
                            {% if Navigation.PreviousPage is not empty %}
                                <a href="{{ Navigation.PreviousPage }}" class="prev button-dark">&laquo; <span>{{ Lang.Get('Previous page') }}</span></a>
                            {% endif %}
                            {% if Navigation.NextPage is not empty %}
                                <a href="{{ Navigation.NextPage }}" class="next button-dark"><span>{{ Lang.Get('Next page') }}</span> &raquo;</a>
                            {% endif %}
                            <div class="clear"></div>
                        </div>
                    {% endif %}
                </section>
            </div>
        </div>
        <!-- shortcut:[Widgets] -->
        <div class="m-bottom">
            <div class="main">
                <section id="content2">
                    {% if Custom('custom_hide_widget_blog_archive') == false and Blog.Archives is not empty %}
                        <!-- Archives -->
                        <article class="widget left">
                            <h3>{{ Lang.Get('Archives') }}</h3>
                            <ul id="archivelist">
                                {% list Blog.Archives %}
                                    <li>
                                        <a href="#" class="plustext" onclick="javascript: return switch_arch('{{ Archive.Year }}')">{{ Archive.Year }}</a>
                                        <ul id="arch_{{Archive.Year}}" class="arch_month">
                                        {% list Archive.Months %}
                                            {% if Month.PostCount > 0 %}
                                                <li><a href="{{ Month.Url }}">{{ Lang.Get(Month.Name) }}</a> ({{ Month.PostCount }})</li>
                                            {% endif %}
                                        {% endlist %}
                                        </ul>
                                    </li>
                                {% endlist %}
                            </ul>
                        </article>
                    {% endif %}

                    {% if Blog.Description is not empty and Custom('custom_hide_widget_blog_about') == false %}
                        <article class="widget left">
                            <h3>{{ Lang.Get('About this blog') }}</h3>
                            {% if Blog.Avatar is not empty %}
                                 <figure class="post-cover">
                                    <a class="avatar" href="{{ Blog.Avatar }}" title="{{ Blog.Title }}">
                                        <img src="{{ Blog.Avatar|resize(226, 100)|crop }}" alt="" width="226" height="100" />
                                    </a>
                                </figure>
                            {% endif %}
                            <p>{{ Blog.Description }}</p>
                        </article>
                    {% endif %}
                    
                    {% if Custom('custom_hide_widget_last_posts') == false %}
                        <!-- Last posts -->
                        <article class="widget left">
                            <h3>{{ Lang.Get('Last posts') }}</h3>
                            {% list Blog.Posts(2) %}
                                <article class="last-post">
                                    <span class="date left">
                                        <span>{{ Post.Date|date("d") }}</span>
                                        {{ Post.Date|datel("MMM") }}
                                    </span>
                                    <a class="title" title="{{ Post.Snippet }}" href="{{ Post.PermaLink }}">
                                        {% if Post.Title is not empty %}
                                            {{ Post.Title }}
                                        {% else %}
                                            {{ Lang.Get('Publihed from') }} 
                                            {% list Post.Sources %}
                                                {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
                                            {% endlist %}
                                        {% endif %}
                                    </a>
                                    <p class="snippet">
                                        {{ Post.Snippet }}
                                    </p>
                                </article>
                            {% endlist %}
                        </article>
                    {% endif %}
                    <div class="clear"></div>
                </section>
            </div>
        </div>
        <!-- shortcut:[Footer] -->
        <footer>
            <div class="main">
                <div class="wrapper">
                    <p>
                        {% if Custom('custom_footer') %}
                            {{ Custom('custom_footer') }}&nbsp;-&nbsp;
                        {% endif %}
                        {{ Lang.Get('Hosted by') }} <a href="http://overblog.com" target="_blank">Overblog</a>
                    </p>
                    <div class="clear"></div>
                </div>
            </div>
        </footer>
        
        <!-- Template scripts -->
        <script type="text/javascript">
            $(document).ready(function() {
                $('#myRoundabout').roundabout({
                     shape          : 'square',
                     minScale       : 0.85, // tiny!
                     maxScale       : 0.95, // tiny!
                     easing         : 'easeOutExpo',
                     clickToFocus   : 'true',
                     focusBearing   : '0',
                     duration       : 750,
                     reflect        : true
                });

                $(".ob-section-images a, .ob-link-img, .avatar").attr("rel", "fancybox");
                $("a[rel=fancybox]").fancybox({
                    'overlayShow'   : true,
                    'titlePosition' : 'over',
                    'transitionIn'  : 'fadin',
                    'transitionOut' : 'fadin',
                    'type'          : 'image'
                });    
                
                (function() {
                    {% if Custom('custom_hide_widget_search_form') == false %}
                        // HTML5 placeholder fallback
                        function isPlaceholderSupported() {
                            'use strict';
                            var o = document.createElement('input');
                            return 'placeholder' in o;
                        }
                        function initPlaceHolder(el){
                            if(jQuery.trim(el.val()) === '' || jQuery.trim(el.val()) === el.attr('placeholder')){
                                el.val(el.attr('placeholder'));
                                el.css({'color': '#999'});
                            }
                        }
                        function togglePlaceholder(el, submitMode) {
                            'use strict';
                            var v = jQuery.trim(el.val()), p = el.attr('placeholder');
                            if (v === '' && !submitMode) {
                                el.val(p);
                                el.css({'color': '#999'});
                            } else if (v === p) {
                                el.val('');
                            }
                        }
                        function emulatePlaceholder(form) {
                            'use strict';
                            if (!isPlaceholderSupported()) {
                                jQuery('input[placeholder]', form).each(function () {
                                    initPlaceHolder(jQuery(this));
                                }).focus(function () {
                                    togglePlaceholder(jQuery(this), false);
                                }).blur(function () {
                                    initPlaceHolder(jQuery(this));
                                });
                                form.submit(function () {
                                    jQuery(this).find('input[placeholder]').each(function () {
                                        togglePlaceholder(jQuery(this), true);
                                    });
                                });
                            }
                        }
                        
                        // Search form
                        function obSearchForm() {
                            'use strict';
                            var i= 0, form = jQuery('form.search');
                            if (form.length) {
                                form.each(function () {
                                    emulatePlaceholder($(this));
                                    var qInput = jQuery('input[name=q]', $(this));
                                    if (qInput.length) {
                                        $(this).submit(function (e) {
                                            e.preventDefault();
                                            window.location = jQuery(this).attr('action') + qInput.val();
                                        });
                                    }
                                });
                            }
                        }
                        obSearchForm();
                    {% endif%}
                    
                    // Archives
                    {% list Blog.Archives %}
                        {% if Archive.Year != "now"|date("Y") %}
                            switch_arch('{{ Archive.Year }}');
                        {% endif %}
                    {% endlist %} 
                })();
                $(".ob-input-submit").addClass("button");
            });
        </script>
        

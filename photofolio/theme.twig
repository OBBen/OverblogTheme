    </head>
    <body id="{% if isIndex %}index{% else %}page2{% endif %}" {% if isSingle %}class="pageSingle"{% endif %}>
        <div id="fb-root"></div>
        <div class="m-top">
            <div class="s-bg"></div>
            <div class="main">
                <!-- shortcut:[Header content] -->
                <header>
                    <div class="wrapper">
                        <!-- Main title -->
                        <h1 id="logo" class="left">
                            <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">
                                {% if Custom('custom_logo_picture') is not empty %}
                                    <img src="{{ Custom('custom_logo_picture')|resize(600,100)|crop }}" alt="{{ Blog.Title }}" title="{{ Blog.Title }}" width="600" height="100" />
                                {% else %}
                                    {{ Blog.Title }}
                                {% endif %}
                            </a>
                        </h1>
                        
                        <!-- Search form -->
                        {% if Custom('custom_hide_widget_search_form') == false %}
                            <form class="search right" id="search" method="get" action="/search/">
                                <div>
                                    <input type="submit" class="submit" value="" />
                                    <input class="input" type="text" name="q" value="{{ Search }}"  placeholder="{{ Lang.Get('Search') }}...">
                                </div>
                            </form>
                        {% endif %}
                        
                        <!--[if lt IE 8]>
                            <div class="clear"></div>
                        <![endif]-->
                        
                        {% if Blog.Description is not empty %}
                            <p id="headline" class="clear">{{ Blog.Description }}</p>
                        {% endif %}
                        
                        <!-- Navigation -->
                        <nav id="menu">
                            <ul>
                                <li{% if isIndex %} class="active"{% endif %}>
                                    <a {% if isIndex %} {% endif %} href="{{ Blog.Url }}">{{ Lang.Get('Home') }}</a>
                                </li>
                                <li {% if isTag %} class="active"{% endif %}>
                                    <a href="#">{{ Lang.Get('Tags') }}</a>
                                    <ul>
                                        {% list Blog.PostsTags(10, 'SORT_POSTS_NUMBER_DESC') %}
                                            <li {% if isTag and Tag.Title == PostsTag.Title %} class="active" {% endif %}>
                                                <a href="{{ PostsTag.Url }}">{{ PostsTag.Title }}</a>
                                            </li>
                                        {% endlist %}
                                    </ul>
                                </li>
                                {% list Blog.ListPages %}
                                    {% if loop.first %}
                                        <li {% if isPage %} class="active"{% endif %}>
                                            <a href="#">{{ Lang.Get('Pages') }}</a>
                                            <ul>
                                    {% endif %}
                                                <li>
                                                    <a href="{{ ListPage.Url }}">{{ ListPage.Title }}</a>
                                                </li>
                                    {% if loop.last %}
                                            </ul>
                                        </li>
                                    {% endif %}
                                {% endlist %}
                                <li class="end">
                                    <a href="/contact">{{ Lang.Get('Contact') }}</a>
                                </li>
                            </ul>
                        </nav>
                        <div class="clear"></div>
                    </div>
                    
                    {% if isIndex %}
                        <!-- Galery -->

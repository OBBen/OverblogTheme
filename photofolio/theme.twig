            .widget .avatar img {border: 7px solid {{ Custom('custom_images_border_color')|default('#FFF') }}; -webkit-box-shadow: 0 25px 30px -30px #000; -moz-box-shadow: 0 25px 30px -30px #000; -ms-box-shadow: 0 25px 30px -30px #000; -o-box-shadow: 0 25px 30px -30px #000; box-shadow: 0 25px 30px -30px #000; margin-bottom: 15px;}
            

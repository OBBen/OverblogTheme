    <body id="{% if isIndex %}page1{% else %}page2{% endif %}"{% if isSingle %} class="pageSingle"{% endif %}>
        <div class="m-top">
            <div class="s-bg"></div>
            <div class="main">
                <!-- shortcut:[Header content] -->
                <header>
                    <div class="wrapper">
                        <!-- Main title -->
                        <h1>
                            <a href="{{ Blog.Url }}" title="{{ Blog.Description }}">
                                {% if Custom('custom_logo_picture') is not empty %}
                                    <img src="{{ Custom('custom_logo_picture')|resize(650,100) }}" alt="{{ Blog.Title|truncate(20, true) }}" title="{{ Blog.Title }}" />
                                {% else %}
                                    {{ Blog.Title|truncate(20, true) }}
                                {% endif %}
                            </a>
                        </h1>
                        
                        <!-- Search form -->
                        {% if Custom('custom_hide_widget_search_form') == false %}
                            <form class="search" id="search" method="get" action="/search/">
                                <div>
                                    <input type="submit" class="submit" value="" />
                                    <input class="input" type="text" name="q" value="{{ Search }}"  placeholder="{{ Lang.Get('Search') }}...">
                                </div>
                            </form>
                        {% endif %}
                        
                        <!--[if lt IE 8]>
                            <div class="clear"></div>
                        <![endif]-->
                        
                        {% if Custom('custom_headline') is not empty %}
                            <!-- Headline -->
                            <p class="m-headline clear">{{ Custom('custom_headline')|truncate(40, true) }}</p>
                        {% endif %}
                        
                        <!-- Navigation -->
                        <nav>
                            <ul id="menu">
                                <!-- Index -->
                                <li{% if isIndex %} class="active"{% endif %}>
                                    <a {% if isIndex %} {% endif %} href="{{ Blog.Url }}">{{ Lang.Get('Home') }}</a>
                                </li>
                                
                                <!-- Tags -->
                                <li {% if isTag %} class="active"{% endif %}>
                                    <a href="#">{{ Lang.Get('Tags') }}</a>
                                    <ul>
                                        {% list Blog.PostsTags(10, 'SORT_POSTS_NUMBER_DESC') %}
                                            <li {% if isTag and Tag.Title == PostsTag.Title %} class="active" {% endif %}>
                                                <a href="{{ PostsTag.Url }}">{{ PostsTag.Title }}</a>
                                            </li>
                                        {% endlist %}
                                    </ul>
                                </li>
                                
                                <!-- Pages -->
                                {% list Blog.ListPages %}
                                    {% if loop.first %}
                                        <li {% if isPage %} class="active"{% endif %}>
                                            <a href="#">{{ Lang.Get('Pages') }}</a>
                                            <ul>
                                    {% endif %}
                                                <li>
                                                    <a href="{{ListPage.Url}}">{{ListPage.Title}}</a>
                                                </li>
                                    {% if loop.last %}
                                            </ul>
                                        </li>
                                    {% endif %}
                                {% endlist %}
                                
                                <!-- Contact -->
                                <li class="end"><a href="/contact">{{ Lang.Get('Contact') }}</a></li>
                            </ul>
                        </nav>
                        <div class="clear"></div>
                    </div>
                    
                    <!-- Galery -->
                    {% if isIndex %}
                        <div class="relative">
                            <div id="gallery">
                                <ul id="myRoundabout">
                                {% if Custom('custom_tag_pictures_pinned') is not empty %}
                                    {% list Blog.TaggedPosts(Custom('custom_tag_pictures_pinned'), 10) %}
                                        {% if TaggedPost.Cover is not empty %}
                                            <li>
                                                <a title="{{ Lang.Get('More about') }} {% if TaggedPost.Title is not empty %}&quot;{{ TaggedPost.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ TaggedPost.PermaLink }}">
                                                    <img src="{{ TaggedPost.Cover|resize(608, 450)|crop }}" alt="" />
                                                </a>
                                            </li>
                                        {% endif %}
                                    {% endlist %}
                                {% else %}
                                    {% list Blog.Posts(10) %}
                                        {% if Post.Cover is not empty %}
                                            <li>
                                                <a title="{{ Lang.Get('More about') }} {% if Post.Title is not empty %}&quot;{{ Post.Title }}&quot;{% else %}{{ Lang.Get('this post') }}{% endif %}" href="{{ Post.PermaLink }}">
                                                    <img src="{{ Post.Cover|resize(608, 450)|crop }}" alt="" />
                                                </a>
                                            </li>
                                        {% endif %}
                                    {% endlist %}
                                {% endif %}
                                </ul>
                            </div>
                        </div>
                    {% endif %}

				background: linear-gradient(top, #ececec 0%,#f8f8f8 2%,#fefefe 3%,#fcfcfc 10%,#d2d2d2 83%,#cccccc 98%,#434a54 100%); /* W3C */
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ececec', endColorstr='#434a54',GradientType=0 ); /* IE6-9 */
				float: none;
			}
			.header .button-icon{
				-moz-border-radius: 5px;
				-o-border-radius: 5px;
				-webkit-border-radius: 5px;
				border-radius: 5px;
				display: block;
				padding: 1px;
			}
			.header .button-icon:active{
				-moz-box-shadow: inset 0 0 4px #000;
				-webkit-box-shadow: inset 0 0 4px #000;
				-o-box-shadow: inset 0 0 4px #000;
				box-shadow: inset 0 0 4px #000;
			}
			.searchForm{
				border: none;
				-moz-box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.4);
				-o-box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.4);
				-webkit-box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.4);
				box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.4);
				-moz-border-radius: 2px;
				-o-border-radius: 2px;
				-webkit-border-radius: 2px;
				border-radius: 2px;
				-moz-transition: margin-top ease-in-out 300ms;
				-webkit-transition: margin-top ease-in-out 300ms;
				-o-transition: margin-top ease-in-out 300ms;
				transition: margin-top ease-in-out 300ms;

			}
			.box,
			.article,
			.post-search,
			.comment{
				background-color: #FFFFFF;
				-moz-border-radius: 4px;
				-o-border-radius: 4px;
				-webkit-border-radius: 4px;
				border-radius: 4px;
				-moz-box-shadow: 0px 2px 4px #656565;
				-webkit-box-shadow: 0px 2px 4px #656565;
				-o-box-shadow: 0px 2px 4px #656565;
				box-shadow: 0px 2px 4px #656565;
				margin: 15px 0;
				overflow: hidden;
			}
			.article,
			.post-search,
			.comment,
			.pagination{
				padding: 1em;
			}
			.box:active {
				-moz-box-shadow: none;
				-webkit-box-shadow: none;
				-o-box-shadow: none;
				box-shadow: none;
			}
			.pagination:active{
				-moz-box-shadow: 0px 2px 4px #656565;
				-webkit-box-shadow: 0px 2px 4px #656565;
				-o-box-shadow: 0px 2px 4px #656565;
				box-shadow: 0px 2px 4px #656565;
			}
			.article{
				margin: 0 0 40px 0;
			}
			.post-search {
				margin: 0 0 15px 0;
			}
			.fb-share,
			.tw-share,
			.gg-share {
				float: left;
				margin: 20px auto;
			}
			.fb-share {margin: 20px 30px 0 0;}
			.list{
				margin: 0;
				padding: 0;
			}
			.list p{
				margin: 0;
			}
			.post-cover{
				float: left;
			}
			.post-cover img{
				margin: 0 10px 0 0;
				padding: 2px;
			}
			.post-cover img,
			li.album .img span{
				border: 1px solid #d0d0d0;
			}
			.contenuArticle{
				overflow: hidden;
			}
			.ob-row-1-col img,
			.ob-row-2-col img,
			.ob-row-3-col img{
				display: block;
				height: auto;
			}

			.ob-video iframe {
				max-width: 100%;
			}
			
			.ob-section-audio .obsoundplayer .obsoundplayername {
				height: 31px;
				width: 240px;
				overflow: hidden;
			}
			.ob-section-map .ob-map {
				max-width: 100%;
				display: block;
				
			}

			.ob-section-file,
			.ob-ctn {
				margin-left: 0;
			}
			.contenuArticle pre {
				width: 100%;
				margin-bottom: 10px;
				white-space: -moz-pre-wrap;
				white-space: -webkit-pre-wrap;
				white-space: -o-pre-wrap;
				white-space: pre-wrap;
				word-wrap: break-word;
				overflow: auto;
			}
			.infoArticle{
				margin: 1em 0 0 0;
			}
			.infoArticle .date{
				float: left;
				font-weight: bold;
				text-transform: uppercase;
			}
			.infoArticle .nbComments{
				float: right;
				font-weight: bold;
				text-transform: uppercase;
			}
			.infoArticle .date,
			.infoArticle .nbComments{
				font-size: 12px;
			}
			.ancre{
				position: relative;
			}
			.ancre a{
				position: absolute;
				top: -50px;
			}
			form.search{
				margin-top: 10px;
				position: relative;
				z-index: 10;
			}
			.search fieldset{
				background: url("http://fdata.over-blog.net/99/00/00/01/images/portal3/mobile/search.png") no-repeat 8px 5px;
				border: 0;
				display: block;
				padding: 2px 60px 2px 35px;
				z-index: 100;
			}
			.search .searchQuery{
				border: 1px solid black;
				-moz-border-radius: 5px;
				-o-border-radius: 5px;
				-webkit-border-radius: 5px;
				border-radius: 5px;
				color: #838383;
				display: block;
				font-size: 1.12em;
				height: 29px;
				padding: 0 3px;
				margin:0;
				width: 100%;
			}
			.search .searchSubmit{
				font-size: 1em;
				height: 26px;
				padding: 0;
				position: absolute;
				right: 11px;
				top: 5px;
				width: 39px;
				z-index: 20;
			}
			.button.submit,
			.search .searchSubmit{
				-webkit-appearance: none;
				background-color: #ff457e;
				   -moz-border-radius: 5px;
					 -o-border-radius: 5px;
				-webkit-border-radius: 5px;
				        border-radius: 5px;
				border: none;
				color: #FFF;
				cursor: pointer;
			}
			.comments ul.list{
				list-style: none;
			}
			.pagination .prev{
				float: left;
			}
			.pagination .next{
				float:	right;
			}
			.pagination a{
				font-weight: bold;
			}
			.bigtitle{
				color: #004664;
			}
			.contact-title {
				padding-left: 20px;
				color: #0072BC;
				font-size: 14px;
			}
			.ob-form {
				margin-top: -10px;
				padding: 1em;
			}
			.ob-label {
				width: 20%;
				margin-left: 2%;
				margin-right: 2%;
			}
			.ob-input {
				width: 100%;
				border-radius: 4px;
				font-size: 12px;
			}
			.ob-input-text, .ob-input-email {
				height: 25px; border: 1px solid #CCC;
			}
			.ob-input-textarea {
				border: 1px solid #CCC;
			}
			.ob-input-submit {
				max-width: 12%;
				height: 25px;
				margin-left: auto;
				margin-right: auto;
				text-align: center;
				border: 0;
				cursor: pointer;
			}
			.ob-input-submit:active {
				-moz-box-shadow: inset 0 0 3px #000;
				-webkit-box-shadow: inset 0 0 3px #000;
				-o-box-shadow: inset 0 0 3px #000;
				box-shadow: inset 0 0 3px #000;
			}
			
            .ob-section-link img{
                padding: 14px 14px 0px 14px;
            }
            
            .ob-section-link .ob-title a{
                 margin: 0 20px;
            }
            
            .ob-section-quote .ob-quote p::before {
                margin: -46px 0px 0px -100px;
            }
            
            .ob-section-quote .ob-quote p::after {
                content: "";
            }
            
            .ob-section-quote .ob-quote p {
                margin: 40px 0px 20px 40px;
            }
            
            .ob-section-quote .ob-author,
            .ob-section-quote .ob-source {
                white-space: nowrap;
                overflow: hidden;
                max-width: 84%;
                text-overflow: ellipsis;
            }
            
            .ob-section-text,
            .ob-section-images,
            .ob-section-video,
            .ob-section-audio,
            .ob-section-quote,
            .ob-section-file,
            .ob-section-html,
            .ob-section-link,
            .ob-video iframe {
                margin: 0px auto 20px auto;
            }
                        
            .ob-section-link .ob-link{
                margin-bottom: 0px;
            }
            
            
            .ob-section-link .ob-img {
                padding:0px;
                display: block;
            }
            
            .ob-section-video iframe {
                padding-bottom: 0px;
                margin-bottom: 0px;
            }
            
            .ob-section .ob-desc {
                font-style:italic;
                margin-top: 6px;
            }
            
            .ob-section-file .ob-ctn {
                width: 99%;
            }
            
            .ob-section-images, 
            .ob-section-text img, 
            .ob-section-video {
            	max-width: 100%;
            }

            .ob-section-text p {
            	text-align: justify;
            }
            .ob-section-images .ob-slideshow,
            .ob-section-images .ob-slideshow .ob-link-img,
            .ob-section-images .ob-slideshow img {width: 100%;}
            .ob-slider-controlNav,
            .ob-slider-directionNav {display: none !important;}
            
		/* end main */

		/* footer */
			.footer{
				font-family: Arial;
				font-size: 12px;
				margin: 0 20px 5px;
				text-align: right;
			}
			.footer ul{
				list-style:none;
				margin-top: 0;
			}
			.footer li{
				display: inline;
				margin-left: 20px;
			}
			.footer li{
				display: inline-block;
				margin-left: 10px;
				margin-right: 10px;
			}
			.footer a{
				color: #565656;
				text-decoration: underline;
			}
			.footer .copyright{
				color: #565656;
				text-align: center;
			}
		/* end footer */
							
		/* iPhone 1 - 2 - 3 - 3GS  (portrait) */
		@media only screen and (max-device-width: 480px) and (orientation : portrait) and (-webkit-min-device-pixel-ratio: 1) {
			.searchForm {margin-top: -9px;}
			.searchForm .searchQuery {width: 75%;}
			.searchForm.hide {margin-top: -50px;}

			.ob-section {max-width: 100%;}
			.ob-section ul {padding-left: 20px;}
			
			.ob-video iframe {max-height: 31%;}

			.ob-section-images img {max-width: 100%;}
			.ob-section-images .ob-slideshow {height: 204px;}
			.ob-section-images .ob-row-3-col img {width: 100%;}

			.ob-media-left,
			.ob-media-right {float: none; margin: 0;}

			.ob-section-map .ob-map {max-height: 272px;}
			.ob-section-map .ob-map > img {max-height: inherit;}

			.ob-section-file .ob-ctn {background: #DCDCDC; max-width: 100%;}
	        .ob-section-file .ob-link {color: #4B4B4B; max-width: 68%;}
	        .ob-section-file .ob-link:hover {color: #222;}
	        
	        .ob-section-quote {background: #DCDCDC;}
	        .ob-section-quote .ob-quote p,
	        .ob-section-quote .ob-quote p:before {color: #4B4B4B;}
	        .ob-section-quote .ob-quote p {margin-left: 0;}
	        .ob-section-quote .ob-quote p:before {font-size: 100px; margin: -50px 0px 0px -50px;}
	        .ob-section-quote .ob-author, 
	        .ob-section-quote .ob-source {background: #EEE; max-width: 78.2%;}

	        .ob-section-link .ob-ctn {background: #DCDCDC; padding: 20px;}
	        .ob-section-link .ob-media-left {float: left; margin: -20px 0 -20px -20px;}
	        .ob-section-link .ob-media-left img {-moz-border-radius: 8px 0 0 8px; -webkit-border-radius: 8px 0 0 8px; -ms-border-radius: 8px 0 0 8px; -o-border-radius: 8px 0 0 8px; border-radius: 8px 0 0 8px; height: 110px; margin: 0 20px 0 0;}
	        .ob-section-link .ob-title {margin: 0; padding: 0;}
	        .ob-section-link .ob-title .ob-link {color: #4B4B4B; font-size: 1.3em; max-height: 22px; margin: 0 0 5px; padding: 0; overflow: hidden;}
	        .ob-section-link .ob-title .ob-link:hover {color: #222;}
	        .ob-section-link .ob-snippet {color: #4B4B4B; margin: 0 0 15px; overflow: hidden; padding: 0; text-overflow: ellipsis; white-space: nowrap;}
	        .ob-section-link .ob-url {background: #EEE; max-width: 44.2%;}
	        .ob-section-link .ob-url .ob-link {color: #444;}
	        .ob-section-link .ob-desc {font-variant: italic;}

	        .fb-share,
	        .tw-share,
	        .gg-share {float: none;}
		}

		/* iPhone 1 - 2 - 3 - 3GS (landscape) */
		@media only screen and (max-device-width: 480px) and (orientation : landscape) and (-webkit-min-device-pixel-ratio: 1) {
			.searchForm {margin-top: -9px;}
			.searchForm .searchQuery {width: 75%;}
			.searchForm.hide {margin-top: -50px;}

			.ob-section {width: 100%;}
			.ob-section ul {padding-left: 20px;}

			.ob-media-left,
			.ob-media-right {float: none; margin: 0;}


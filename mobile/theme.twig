	<body class="yui-skin-ob __blog __mobile fr FR" >
		<div class="header clearfix">
			<a class="logo" href="{{ Blog.Url }}" title="{{ Blog.Title }}"></a>
			<div class="topLinks">
				<div class="button-box">
		            <a class="button-icon search" href="#" title="{{ Lang.Get('Search') }}">
		                <span class="icon"></span>
		            </a>
				</div>
			</div>

            <div class="topLinks">
				<div class="button-box">
		            <a class="button-icon home" href="{{ Blog.Url }}" title="{{ Lang.Get('Back') }}">
		                <span class="icon"></span>
		            </a>
				</div>
			</div>
		</div>
		<form class="searchForm hide" name="search" action="/search" method="get">
			<input id="" class="searchQuery" name="q" type="text" placeholder="{{ Lang.Get('Search') }}..." />
			<input class="searchSubmit" type="submit" value="OK" />
		</form>

		<div class="main ">
			<h1 class="h1">{{ Blog.Title }}</h1>
			{% if isIndex %}
				<div>
					<ul class="list">
						{% list Posts %}
							<li class="element clearfix box">
								<h2 class="h2 article-sample clearfix">
									<a href="{{ Post.Permalink }}" title="{{ Post.Title }}">
										<span class="article-inner">
											{% if Post.Cover is not empty %}
												<span class="post-cover">
													<img src="{{ Post.Cover|resize(100,100)|crop }}" title="{{ Post.Title }}" alt="{{ Post.Title }}" />
												</span>
											{% endif %}
											<span class="post-title">
												{% if Post.Title is not empty %}
	                                                {{ Post.Title }}
	                                            {% else %}
	                                                {{ Lang.Get('Posted from') }} 
	                                                {% list Post.Sources %}
	                                                    {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
	                                                {% endlist %}
	                                            {% endif %}
											</span>
											<span class="post-snippet sample text_mobile">
												{{ Post.Snippet }}
											</span>
											<span class="clear"></span>
										</span>
									</a>
								</h2>
							</li>
						{% endlist %}
					</ul>
				</div>
			{% elseif isSingle %}
				{% list Posts %}
					<div class="article">
						<div class="divTitreArticle">
							<h2 class="h2">
								<a href="{{ Post.Permalink }}" title="{{ Post.Title }}">
									{% if Post.Title is not empty %}
                                        {{ Post.Title }}
                                    {% else %}
                                        {{ Lang.Get('Posted from') }} 
                                        {% list Post.Sources %}
                                            {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
                                        {% endlist %}
                                    {% endif %}
								</a>
							</h2>
						</div>
						<div class="contenuArticle">
							{{ Post.Body }}
							<div class="clear center"></div>
						</div>
						<div class="infoArticle clearfix">
							<span class="date">
								{{ Post.Date|datel(Lang.Get('Default date format'))}}
							</span>
							<div class="clear"></div>
							<div class="fb-share">
								<div class="fb-like" data-href="{{ Post.Permalink }}" data-send="false" data-layout="button_count" data-show-faces="false" data-font="lucida grande" data-width="90"></div>
							</div>
							<div class="tw-share">
								<a href="https://twitter.com/share" class="twitter-share-button" data-text="{% if Post.Title is not empty %}{{ Post.Title }}{% elseif Post.Snippet is not empty %}{{ Post.Snippet}}{% else %}{{ Blog.Title }}{% endif %}" data-url="{{ Post.Permalink }}" data-lang="{{ Blog.Lang }}" {% if Custom('twitter_account') is not empty %} data-via="{{Custom('twitter_account')}}"{% endif %}></a>
							</div>
							<div class="gg-share">
								<g:plusone size="medium"></g:plusone>
							</div>
						</div>
					</div>
					{% if Post.ShowComments is not empty %}
						{% if Custom("comment") == "Facebook" %}
							<div class="comment">
								<div class="fb-comments" data-href="{{ Post.Permalink }}" data-num-posts="2" data-width="620"></div>
							</div>
						{% elseif Custom("comment") == "Disqus" and Custom('comment_disqus') is not empty %}
							<div class="comment">
								<div id="disqus_thread"></div>
								<script type="text/javascript">
									var disqus_shortname = '{{ Custom('comment_disqus') }}';
			
									(function() {
										var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
										dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
										(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
									})();
								</script>
								<noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
							</div>
						{% else %}
							<div class="comment">
								{{ Post.Comments }}
							</div>
						{% endif %}
					{% endif %}
				{% endlist %}
			{% elseif isSearch %}
				<div class="post-search">
					<div class="divTitreArticle">
						<h2 class="h2">
							{{ Lang.Get('Search results for') }} "{{ Search.Keyword }}"
						</h2>
					</div>
					{% if isSearch and Posts is empty %}
						<p>{{ Lang.Get('No results for this expression.') }}</p>
					{% endif %}
				</div>
				<div>
					<ul class="list">
						{% list Posts %}
							<li class="element clearfix box">
								<h2 class="h2 article-sample clearfix">
									<a href="{{ Post.Permalink }}" title="{{ Post.Title }}">
										<span class="article-inner">
											{% if Post.Cover is not empty %}
												<span class="post-cover">
													<img src="{{ Post.Cover|resize(100,100)|crop }}" title="{{ Post.Title }}" alt="{{ Post.Title }}" />
												</span>
											{% endif %}
											<span class="post-title">
												{% if Post.Title is not empty %}
	                                                {{ Post.Title }}
	                                            {% else %}
	                                                {{ Lang.Get('Posted from') }} 
	                                                {% list Post.Sources %}
	                                                    {{ Lang.Get(Source) }} {% if not loop.last %}{{ Lang.Get('and') }} {% endif %}
	                                                {% endlist %}
	                                            {% endif %}
											</span>
											<span class="post-snippet sample text_mobile">
												{{ Post.Snippet }}
											</span>
											<span class="clear"></span>
										</span>
									</a>
								</h2>
							</li>
						{% endlist %}
					</ul>
				</div>
			{% elseif isSpecial %}
				<div class="box clearfix">
					<h2 class="contact-title">{{ Lang.Get('Contact us') }}</h2>
					<form class="ob-form" action="" method="post">
						<div class="ob-form-field">
							<input id="form_name" class="ob-input ob-input-text" type="text" name="form[name]" placeholder="Name" required="required" />
						</div>
						<div class="ob-form-field">
							<input id="form_email" class="ob-input ob-input-email" type="email" name="form[email]" placeholder="Email" required="required" />
						</div>
						<div class="ob-form-field">
							<input id="form_title" class="ob-input ob-input-text" type="text" name="form[title]" placeholder="Title" required="required" />
						</div>
						<div class="ob-form-field">
							<textarea id="form_message" class="ob-input ob-input-textarea" name="form[message]" placeholder="Message" required="required"></textarea>
						</div>
						<input type="submit" class="ob-input ob-input-submit" value="{{ Lang.Get('Send') }}" />
					</form>
				</div>

			{% endif %}
			{% if Navigation.PreviousPage is not empty or Navigation.NextPage is not empty or Navigation.PreviousPost is not empty or Navigation.NextPost is not empty %}
				<div class="box pagination clearfix">
					{% if Navigation.PreviousPage is not empty %}
						<a class="prev" href="{{ Navigation.PreviousPage }}">&lt; {{ Lang.Get('Previous Page') }}</a>
					{% elseif Navigation.PreviousPost is not empty %}
						<a class="prev" href="{{ Navigation.PreviousPost.Permalink }}">&lt; {{ Lang.Get('Previous Post') }}</a>
					{% endif %}
					{% if Navigation.NextPage is not empty %}
						<a class="next" href="{{ Navigation.NextPage }}">{{ Lang.Get('Next Page') }} &gt;</a>
					{% elseif Navigation.NextPost is not empty %}
						<a class="next" href="{{ Navigation.NextPost.Permalink }}">{{ Lang.Get('Next Post') }} &gt;</a>
					{% endif %}
				</div>
			{% endif %}

		</div>

		<div class="footer">
			<ul>
				<li>
					<a href="{{ Blog.Url }}/contact">{{ Lang.Get('Contact us') }}</a>
				</li>
				<li>
					<a href="" class="noMob">{{ Lang.Get('Classic site') }}</a>
				</li>
			</ul>
		</div>

		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
		<script type="text/javascript">
			$('.noMob').click(function() {
                document.cookie = "_noMob=1;path=/";
                window.location.reload();
                return false;
			});
		</script>

		<script type="text/javascript">
			$('.search').click(function() {
				$('.searchForm').toggleClass('hide');
			    $('.searchQuery').focus();
			    return false;
			});
			$(document).ready(function() {
				$('.ob-slideshow').removeClass('.ob-slideshow');
			});

		</script>

		<!-- Facebook Script -->
		<div id="fb-root"></div>
		<script type="text/javascript">
		    (function(d, s, id) {
		        var js, fjs = d.getElementsByTagName(s)[0];
		        if (d.getElementById(id)) return;
			        js = d.createElement(s); js.id = id;
			        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('127607930619601')}}";
			        fjs.parentNode.insertBefore(js, fjs);
		    }(document, 'script', 'facebook-jssdk'));
		</script>

		<!-- Twitter button -->
        <script>
        	!function(d,s,id){
        		var js, fjs = d.getElementsByTagName(s)[0];
        		if(!d.getElementById(id)){
	        		js = d.createElement(s);
	        		js.id = id;
	        		js.src = "//platform.twitter.com/widgets.js";
	        		fjs.parentNode.insertBefore(js,fjs);
	        	}
	        }(document,"script","twitter-wjs");
        </script>

		<!-- Google + button -->
		<script type="text/javascript">
		  window.___gcfg = {lang: '{{ Blog.Lang }}'};
		  (function() {
		      var po = document.createElement('script');
		      po.type = 'text/javascript';
		      po.async = true;
		      po.src = 'https://apis.google.com/js/plusone.js';
		      var s = document.getElementsByTagName('script')[0];
		      s.parentNode.insertBefore(po, s);
		  })();
		</script>
	</body>
</html>
